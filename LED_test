/ Teste Simples em Arduino

// Definir o pino do LED
const int ledPin = 13;

void setup() {
  // Inicializar o pino do LED como sa√≠da
  pinMode(ledPin, OUTPUT);
}

void loop() {
  // Acender o LED
  digitalWrite(ledPin, HIGH);
  delay(1000); // Aguardar 1 segundo

  // Apagar o LED
  digitalWrite(ledPin, LOW);
  delay(1000); // Aguardar 1 segundo
}
